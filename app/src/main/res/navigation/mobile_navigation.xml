<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_user"
        android:name="com.example.codeforces.ui.UserFragment"
        android:label="@string/title_users"
        tools:layout="@layout/fragment_user"
        />

    <fragment
        android:id="@+id/navigation_contests"
        android:name="com.example.codeforces.ui.ContestsFragment"
        android:label="@string/title_contests"
        tools:layout="@layout/fragment_contests" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.codeforces.ui.ProblemsFragment"
        android:label="@string/title_problems"
        tools:layout="@layout/fragment_problems" />
</navigation>
